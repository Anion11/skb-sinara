                    +ui-p2('Должна содержать:', 'parameters, --bold')
                    +ui-ul(listItems2, 'parameters')
                    +ui-p3('Срок действия: 6 месяцев')
                  +ui-accordion({p2: 'Справка по форме банка'})
                  +ui-accordion({p2: 'Справка по форме ФНС'})
                  +ui-accordion({p2: 'Выписка по счёту'})
                  +ui-accordion({p2: 'Декларация 3-НДФЛ'})
                  +ui-accordion({p2: 'Подтверждение, что вы получаете пенсию менее 12 месяцев'})
                  +ui-accordion({p2: 'Выписка со счёта для зачисления пенсии'})
                  +ui-accordion({p2: 'Справка организации, назначившей пенсию'})
                .parameters__dosc
