mixin parameters(mods)
  -
    const baseName = 'parameters'
    let allMods = ''
    if (typeof mods !== 'undefined' && mods) {
      const modsList = mods.split(',')
      for (const element of modsList) {
        allMods += element.trim()[0] === '-' ? ' ' + baseName + element.trim() : ' ' + element.trim() + '__' + baseName
      }
      allMods = allMods.trim()
    }
    const tabHeadings = [
      { p4: 'Условия кредитования' },
      { p4: 'Cпособы погашения кредита' },
      { p4: 'Cпособы подтверждения дохода' },
      { p4: 'Тарифы и документы' }
    ]
    const cardsGoodnessTabHeadings = [
      { p4: 'Условия по карте' },
      { p4: 'Тарифы и документы' },
      { p4: 'Вопросы и ответы' }
    ]
    const cardsSalaryTabHeadings = [
      { p4: 'Условия по карте' },
      { p4: 'Тарифы и документы' }
    ]
    const listItemsMobilization = [
      { p3: 'призваны на военную службу по мобилизации;' },
      { p3: 'проходите военную службу по контракту, находитесь на военной службе в войсках национальной гвардии РФ, в воинских формированиях и органах, при условии участия в СВО на территориях Украины, ДНР, ЛНР, Херсонской и Запорожской областей;' },
      { p3: 'являетесь сотрудником пограничной службы, находящимся на территории России и обеспечивающим проведение СВО;' },
      { p3: 'Гражданство – Российская Федерация' }
    ]
    const listItems1 = [
      { p3: 'Возраст заемщика — от 23 до 80 лет на момент окончания срока действия кредитного договора' },
      { p3: 'Стаж — не менее 3 месяцев на последнем месте работы' },
      { p3: 'Регистрация и проживание — кредитуемые Субъекты РФ' },
      { p3: 'Гражданство – Российская Федерация' }
    ]
    const listItems2 = [
      { p3: 'данные лица, которому выдан документ (ФИО, дата рождения);' },
      { p3: 'наименование работодателя, сумма выплат и иных вознаграждений за последние 12 месяцев (при стаже работы менее 12 месяцев - за фактическое время работы, минимум - 3 месяца);' },
      { p3: 'на последней странице - штамп с указанием реквизитов усиленной квалифицированной электронной подписи' }
    ]
    const cardsGoodnessLines = [
      {
        name: 'Стоимость выпуска карты',
        descr: '0 ₽/мес.'
      },
      {
        name: 'Стоимость обслуживания карты, в месяц',
        descr: '100 ₽'
      },
      {
        name: 'Полный пакет «SMS-информирование»',
        descr: '0 ₽'
      },
      {
        name: 'Кешбэк',
        descr: '1.3%'
      },
      {
        name: 'Переводы с использованием Системы быстрых платежей',
        list: [
          { p3: 'До 100 000 ₽/мес. – 0%' },
          { p3: 'Свыше 100 000 ₽ /мес., но не более 150 000 ₽ /сутки – 0,5%, max 1 500 ₽' },
        ]
      },
      {
        name: 'Выдача наличных денежных средств в банкоматах ПАО Банк Синара, АО «Газэнергобанк», ПАО Банк «ФК Открытие», АО «Альфа-Банк»',
        descr: '0 ₽'
      },
    ]
    const cardsPremiumLines = [
      {
        name: 'Cash-back (возврат средств за оплату товаров и услуг картой)',
        descr: '1%, но не более 50 000 руб./ежемесячно'
      },
      {
        name: 'Дополнительные карты',
        descr: 'Категории Classic / Gold / Signature / Mir Supreme / Mir Supreme Digital'
      },
      {
        name: 'SMS-информирование об операциях',
        descr: 'Бесплатно'
      },
      {
        name: 'Выдача наличных в любых банкоматах',
        descr: 'Бесплатно'
      },
      {
        name: 'Подключение и обслуживание Интернет-банк',
        descr: 'Бесплатно'
      },
      {
        name: 'Переводы по системе Интернет-банк',
        descr: 'Бесплатно'
      },
      {
        name: 'Зачисление средств на карту (p2p перевод)',
        descr: 'Бесплатно'
      },
      {
        name: 'Справка о состоянии счетов',
        descr: 'Бесплатно'
      },
      {
        name: 'Предоставление выписки по счету за любой период',
        descr: 'Бесплатно'
      },
    ]
    const cardsUniversalLines = [
      {
        name: 'Выпуск карты',
        descr: 'Бесплатно, 450 ₽ – на пластиковом носителе к цифровой карте Мир'
      },
      {
        name: 'Обслуживание карты',
        descr: 'Бесплатно',
        list: [
          { p3: 'в первый месяц, в котором была получена карта;' },
          { p3: 'при наличии действующего потребительского кредита;' },
          { p3: 'при наличии действующего срочного вклада*;' },
          { p3: 'при остатке не менее 30 000 ₽ и покупках от 15 000 ₽**;' },
          { p3: 'при отсутствии операций по карте.' },
