      .parameters(class=allMods)&attributes(attributes)
        .parameters__head
          +ui-h3('Параметры', 'parameters, --bold')
        .parameters__item
          .parameters__lines.parameters__lines--mb30.parameters__lines--scroll
            .parameters__line.parameters__line--head.parameters__line--1col-xs
