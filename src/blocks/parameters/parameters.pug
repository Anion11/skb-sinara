                +ui-button('', '--arrow-right')
        +ui-tab(data-tab-group='card-parameters')
          +ui-tab-body('parameters')
            +ui-tab-item('--active')(data-scroll-lock-scrollable)
              .parameters__body.parameters__body--grid-x3
                +ui-h4('Тарифы и документы', 'parameters, --bold')
                .parameters__item
                  +ui-p3('Тип', 'parameters, --secondary3')
                  +ui-p1('Дебетовая карта')
                .parameters__item
                  +ui-p3('Тарифный план', 'parameters, --secondary3')
