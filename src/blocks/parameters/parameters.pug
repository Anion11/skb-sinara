    when 'cards-universal'
      .parameters(class=allMods)&attributes(attributes)
        .parameters__head
          +ui-h3('Параметры', 'parameters, --bold')
          +ui-tab-head('', 'parameters')(data-tab-group='card-parameters')
            each item, index in [ { p4: 'Условия по карте' }, { p4: 'Тарифы и документы' } ]
