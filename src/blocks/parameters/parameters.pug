                  +ui-p3('Валюта', 'parameters, --secondary3')
                  +ui-p1('Рубли РФ (₽)')
                .parameters__item.parameters__item--w100.parameters__item--pd-l
                  each item in cardsUniversalLines
                    .parameters__line.parameters__line--1col-xs
                      +ui-p3(item.name, '--secondary3')
                        if item.nameTooltip
                          +ui-tooltip('', 'tt')
                      +ui-p3('')
                        if item.descr
                          p !{item.descr}
                        if item.list
                          +ui-ul(item.list)
                        if item.descrBot
                          p !{item.descrBot}
