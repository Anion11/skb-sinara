      {
        name: 'Снятие наличных',
        descr: 'Бесплатно',
        list: [
          { p3: 'в банкоматах ПАО Банк Синара, АО «Газэнергобанк»' },
        ]
      },
      {
        name: 'Лимиты на снятие',
        list: [
          { p3: '100 000 ₽/сут. - в банкоматах ПАО Банк Синара, АО «Газэнергобанк»' },
          { p3: '1 000 000 ₽/сут. – в кассах ПАО Банк Синара и АО «Газэнергобанк»' },
        ]
