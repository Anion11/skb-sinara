      {
        name: 'Переводы в интернет-банке и мобильном приложении',
        descr: 'С использованием Системы быстрых платежей:',
        list: [
          { p3: 'в банкоматах ПАО Банк Синара, АО «Газэнергобанк»' },
        ]
      },
    ]

  case pageName
    when 'cards-yes'
      .parameters(class=allMods)&attributes(attributes)
        .parameters__head
          +ui-h3('Параметры', 'parameters, --bold')
          +ui-p1('Распоряжайтесь и управляйте кредитом так, как вам удобно!')
          +ui-tab-head('', 'parameters')(data-tab-group='credit-parameters')
            each item, index in tabHeadings
              .ui-tab-head__item(class=(index === 0 ? 'ui-tab-head__item--active' : ''))
                +ui-p4(item.p4)
                +ui-button('', '--arrow-right')
        +ui-tab(data-tab-group='credit-parameters')
          +ui-tab-body('parameters')
            +ui-tab-item('--active')(data-scroll-lock-scrollable)
              +ui-h4('Условия кредитования', 'parameters, --bold')
              .parameters__body.parameters__body--grid-x2
                .parameters__item
                  +ui-p3('Сумма на руки', 'parameters, --secondary3')
                  +ui-p1('от 51 000 до 3 000 000 ₽')
                .parameters__item
                  +ui-p3('Срок кредита', 'parameters, --secondary3')
                  +ui-p1('от 4 до 5 лет')
                .parameters__item.parameters__item--x2column
                  .parameters__line
                    +ui-p3('Валюта', '--secondary3')
                    +ui-p3('Рубли')
                  .parameters__line
                    +ui-p3('Выпуск/перевыпуск карты', '--secondary3')
                    +ui-p3('Бесплатно')
                  .parameters__line
                    +ui-p3('Процентная ставка', '--secondary3')
                    +ui-ul(listItems1)
              .parameters__close
            +ui-tab-item()(data-scroll-lock-scrollable)
              +repayment([{withoutCommission: true}], 'parameters')
              +repayment([{withCommission: true}], 'parameters')
              .parameters__close
            +ui-tab-item()(data-scroll-lock-scrollable)
              .parameters__body
                +ui-h5('Подтвердите свой доход за несколько минут', 'parameters, --bold')
                .parameters__accordions
                  +ui-accordion({p2: 'Выписка о состоянии счёта ПФР'})
                    +ui-p2('Должна содержать:', 'parameters, --bold')
                    +ui-ul(listItems2, 'parameters')
                    +ui-p3('Срок действия: 6 месяцев')
                  +ui-accordion({p2: 'Справка по форме банка'})
                  +ui-accordion({p2: 'Справка по форме ФНС'})
                  +ui-accordion({p2: 'Выписка по счёту'})
                  +ui-accordion({p2: 'Декларация 3-НДФЛ'})
                  +ui-accordion({p2: 'Подтверждение, что вы получаете пенсию менее 12 месяцев'})
                  +ui-accordion({p2: 'Выписка со счёта для зачисления пенсии'})
                  +ui-accordion({p2: 'Справка организации, назначившей пенсию'})
                .parameters__dosc
                  +document({p4: 'Шаблон справки по форме банка'})
                  +document({p4: 'Инструкция по получению Извещения о состоянии счета ПФР'})
              .parameters__close
            +ui-tab-item()(data-scroll-lock-scrollable)
              .parameters__body
                +ui-h4('Тарифы и документы', 'parameters, --bold')
                +document({p4: 'Тарифы банка'})
                +document({p4: 'Заявление о присоединении к Правилам комплексного банковского обслуживания и подключении услуг'})
                +document({p4: 'Реквизиты для безналичного пополнения счета'})
                +document({p4: 'Правила комплексного банковского обслуживания физических лиц в ПАО Банк Синара. Версия 8.4'})
                +document({p4: 'Правила КБО Версии 1.0-8.3'})
                +document({p4: 'Условия предоставления и использования потреб. кредитов в ПАО Банк Синара'})
                +document({p4: 'Способы и адреса для направления обращений получателями финансовых услуг'})
                +document({p4: 'Шаблон справки о заработной плате по форме банка'})
              .parameters__close
        block
    when 'cards-goodness'
      .parameters(class=allMods)&attributes(attributes)
        .parameters__head
          +ui-h3('Параметры', 'parameters, --bold')
          +ui-tab-head('', 'parameters')(data-tab-group='credit-parameters')
            each item, index in cardsGoodnessTabHeadings
              .ui-tab-head__item(class=(index === 0 ? 'ui-tab-head__item--active' : ''))
                +ui-p4(item.p4)
                +ui-button('', '--arrow-right')
        +ui-tab(data-tab-group='credit-parameters')
          +ui-tab-body('parameters')
            +ui-tab-item('--active')(data-scroll-lock-scrollable)
              .parameters__body
                +ui-h4('Условия по карте', 'parameters, --bold')
                .parameters__item
                  each item in cardsGoodnessLines
                    .parameters__line.parameters__line--1col-xs
                      +ui-p3(item.name, '--secondary3')
                      if item.descr
                        +ui-p3(item.descr)
                      if item.list
                        +ui-ul(item.list)
              .parameters__close
            +ui-tab-item()(data-scroll-lock-scrollable)
              .parameters__body
                +ui-h4('Тарифы и документы', 'parameters, --bold')
                +document({p4: 'Тарифы банка'})
                +document({p4: 'Заявление о присоединении к Правилам комплексного банковского обслуживания и подключении услуг'})
                +document({p4: 'Реквизиты для безналичного пополнения счета'})
                +document({p4: 'Правила комплексного банковского обслуживания физических лиц в ПАО Банк Синара. Версия 8.4'})
                +document({p4: 'Правила КБО Версии 1.0-8.3'})
                +document({p4: 'Условия предоставления и использования потреб. кредитов в ПАО Банк Синара'})
                +document({p4: 'Способы и адреса для направления обращений получателями финансовых услуг'})
